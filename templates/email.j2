<html><head>
<meta http-equiv="Content-Type" content="text/html; charset=us-ascii"><style>
			body
			{
			background-color: #ccccc; 
			font: Arial, sans-serif;
			font-size: 12px;
			width: 400px;
			color: #333333;
			}
			.main
			{
			background-color: white;
			margin:8px;
			font: 12px Arial, sans-serif;
			padding-left: 8px;
			}

			h1
			{
			color:  #cc6600;
			padding: 2px;
			font: 18px Arial, sans-serif;
			border-bottom: solid 1px #7288ac;
			margin-top: 2px;
			}

			h2
			{
			color:  #7288ac;
			font-size: 14px;
			}

			.ribbon
			{

			}

			.ribbont
			{
			border-collapse: collapse; 
			width: 400px;
			font: 10px Arial, sans-serif;
			vertical-align: top;
			text-align:center; 
			background-color: #E5E9EE;
			border: 1px solid #b5b5b5;
			color: #3B3B3B;
			width: 400px;
			}

			.ribbon TD
			{
			font: 10px Arial, sans-serif;
			border-left: 1px solid #b5b5b5;
			border-right: 1px solid #b5b5b5;
			padding: 2px 2px 2px 2px;
			vertical-align:top;
			width: 120px;
			}

			.ribbon A
			{
			font: 14px Arial, sans-serif;
			}

			.ribbon .header
			{
			font: 10px Arial, sans-serif;
			text-align:center;
			}

			.ribbon TD
			{
			background-color:#F2F2F7; 
			}

			.ribbon h4
			{
			color: #7288ac;
			font: 12px Arial, sans-serif;
			margin: 0 0 0 0;
			padding: 0 0 0 0;
			}

			
			.grid
			{
			font: 11px Arial, sans-serif;
			border: solid 1px #898d8f;
			border-collapse: collapse !important;
			text-align: left;
			}

			.headeritem 
			{
			color:  white;
			background-color: #7288ac; 
			padding: 2px;
			font: 14px Arial, sans-serif;
			border-bottom: solid 1px #cccccc;
			}	

			.headeritem TD
			{
			padding-left: 2px;
			padding-right: 8px;
			}

			.griditem	
			{
			height: 20px;
			font: 11px Arial, sans-serif;
			border-collapse: collapse;
			}	

			.griditem TD
			{
			padding-left: 2px;
			padding-right: 2px;
			padding-top: 2px; 
			padding-bottom: 1px;
			vertical-align: top;	
			}

			.footer
			{
			font: 11px Arial, sans-serif;
			color: #666;
			margin: 8px 8px 8px 8px;
			}

			.note
			{
			font: 10px Arial, sans-serif;
			color:  #7288ac;
			}

			.jobdetails
			{
			padding-top: 3px;
			}

			.jobdetails TABLE
			{
			color:  #22222;
			border-top: solid 3px #2d3741;
			width:100%;
			background-color: #F2F2F7;
			}

			.jobdetails TD
			{
			font: 10px Arial, sans-serif;
			white-space: nowrap;
			}

			.ServiceInstance
			{
			border: solid 1px #cccccc;
			display: block;
			white-space: normal;
			}

			.ServiceInstance .disabled
			{
			color: Gray;
			background-color: #F6F6F6;
			}	
			.ServiceInstance .content
			{
			padding: 4px;
			}
			.ServiceInstance .deleted
			{
			background-color: #F6F6F6;
			text-decoration: line-through;
			}

			.ServiceInstance .header
			{
			background-color: #efefef; 
			padding: 4px;
			border-bottom: solid 1px #cccccc;
			font: 10px Arial;
			}

			.ServiceInstance h2
			{
			vertical-align: top;
			color:  #ED8029;
			font: 16px Arial;
			display: block;
			font-weight: normal;
			margin: 0 0 0 0;  
			}

 
			.ServiceInstance  h3
			{
			font: 14px Arial;
			display: block;
			border-bottom: solid 1px #cccccc;
			font-weight: normal;
			margin: 0 0 0 0;
			padding: 4px;
			}

			del
			{
			padding-right: 4px;
			color: red;
			}

			.readonly
			{
			font: 11px Arial;
			border-right: #cccccc 1px solid;
			border-top: #cccccc 1px solid;
			border-left: #cccccc 1px solid;
			border-bottom: #cccccc 1px solid;
			background-color: Transparent;    
			padding: 2px 2px 2px 2px;
			line-height: 16px;
			min-width: 100px;
			display: inline-block;
			margin-bottom: 2px;
			}

			.readonly del
			{
			padding-right: 16px;
			color: #cccccc;
			}

			.radiop
			{
			height: 16px;
			width: 16px;
			font-family: Wingdings;
			font-size: 12pt;
			}

			.checkboxp
			{
		
			height: 16px;
			width: 16px;
			font-size: 12pt;
			}

			.checkboxl
			{
			float: left;
			}
			.checkboxr
			{
			display: inline-block;
			}

			.formc
			{
			display: block;
			width: 100%;
			font: 12px Arial, sans-serif;
			}

			.formc TABLE
			{
			border-collapse: collapse;
			white-space: nowrap;
			padding: 1px 1px 1px 1px;
			font: 11px Arial, sans-serif;
			}

			.formc DIV
			{
			vertical-align: top;
			}

			.formlabel
			{
			font: 11px Arial, sans-serif;
			display: inline-block;
			padding-right: 12px;
			white-space: normal;
			}

			.formlabelr
			{
			font: 11px Arial, sans-serif;
			display: inline;
			}

			.formlabelrequired
			{
			font: 11px Arial, sans-serif;
			color: Navy;
			background-image: url(required.gif);
			background-position: right 4px;
			background-repeat: no-repeat;
			display: inline-block;
			white-space: normal;
			float: left;
			padding-right: 12px;
			}

			/* Used to display constant data to the user */
			.formlabelconstant
			{
			background-color: #F6F6F6;
			border: 1px solid #898d8f;
			padding: 1 16 1 4;
			display: inline-block;
			float: left;
			}

			DIV.listPanelr 
			{
			font: 11px Arial, sans-serif;
			border: 1px solid #cccccc;
			}

			.listPanelr h4
			{
			font: 11px Arial, sans-serif;
			margin: 0 0 0 0;
			border-bottom: 1px solid #cccccc;
			}			

			/* Old styles - Legacy 4.x */
			.serviceItem {  border: solid 1px #cccccc;}
			.serviceHeader {   color:  white;   background-color: #7288ac;    padding: 2px;  font-size: 14px;   font-weight: bold;   border-bottom: solid 1px #cccccc;}
			.serviceDetail { padding: 4px; }

			</style>
	</head>
	<body>
		<h1>{{email.header}}</h1>
		<div class="main">
			<p>{{email.title}}</p>
			<p><em>{{email.description}}</em><br><br>

			{% if email.box_title is defined and email.box_content is defined and email.scanned is defined and email.scanned == true %}
				{% include "email_box.j2" ignore missing %}				
			{% endif %}	 

			{% if getit is defined %}
				{% include "getit_footer.j2" ignore missing %}
			{% endif %}
		</div>
	</body>
</html>

